  "scripts": {
    "start": "bun run src/relay.ts",
    "dev": "bun run --watch src/relay.ts",
    "agent": "bun run src/agent-loop.ts",
    "deep-think": "bun run src/deep-think.ts",
    "goal-engine": "bun run src/goal-engine.ts",
